from cs50 import get_int

def main():
    a = get_int("La valeur de l'entier a est :\n")
    b = get_int("La valeur de l'entier b est :\n")
    print(f"De base a vaut {a} et b vaut {b} \n")
    swap(a, b)
    print(f"DÃ©sormais a vaut {b} et b vaut {a} \n")

def swap(c, d):
    tmp = c
    c = d
    d = tmp

main()
